{
    "playground":{
	"shapes":{
		"Crosstalk_matrix_box": {
			"mean": 0,
			"stdev": 0.5,
			"max": [-1000, -1000, -1000, -1000, -1000]
		}
	},
	"dot_locations": [[0, 1]],
	"fake_dot_tolerances": [40],
	"ndim": 5,
        "plot":false},

    "plunger_gates": [1],
    "save_dir":"mock_device_demo/",

    "investigation":{
	"measurement_seq":["diag_trace", "do_nothing"],
	"cond_meas": [false, false],
	"diag_trace":{
	    "func":"mock_measurement",
	    "condition":"sub_peak_check",
	    "a":[0,0],
	    "b":[-1000,-1000],
	    "pause": 0.1,
	    "verbose":true},
	"do_nothing":{
		"func": "do_nothing",
		"condition": "check_nothing"
	}},

    "detector": {
	"d_r": 2,
	"len_after_poff": 300,
	"th_high": 0.2,
	"th_low": 0.01},

    "general": {
	"directions": [-1.0, -1.0, -1.0],
	"lb_box": [-3000, -3000, -3000, -3000, -3000],
	"bound": [-3000, -3000, -3000, -3000, -3000],
	"num_samples": 40,
	"origin": [0, 0, 0, 0, 0],
	"ub_box": [2000, 2000, 2000, 2000, 2000]},

	"subsample": {
	"sub_ndim": 2,
	"sub_directions": [-1.0, -1.0],
	"sub_lb_box": [-3000, -3000],
	"sub_bound": [-3000, -3000],
	"num_sub_samples":30,
	"sub_ub_box": [2000, 2000],
	"sub_origin": [0, 0],
	"d_tooclose": 2},


    "gpc": {
	"gpc_start": 10,
	"gpc_on": false,
	"gpc_list": [true, true],
	"configs": {
	    "length_prior_mean": 500.0,
	    "length_prior_var": 100.0,
	    "var_prior_mean": 50.0,
	    "var_prior_var": 20.0,
	    "kernal": "Matern52"}
	},

	"cs_compensation": {
		"cs_gates": ["c4", "c5", "c6"],
		"device_gates": ["c8", "c9", "c10", "c11", "c12", "c13", "c14"],
		"characterisation_range": [0, 2000],
		"ml_model": "scikit_linear"
	},

    "gpr": {
	"restarts": 5,
	"factor_std": 2.0,
	"gpr_start": 10,
	"gpr_on": false,
	"length_prior_mean":0.4,
	"length_prior_var" : 0.1,
	"r_min": 0.0,
	"var_prior_mean_divisor": 4.0,
	"kernal": "Matern52"},

    "pruning": {
	"pruning_stop": 30,
	"pruning_on": false,
	"step_back": 100},

    "sampling": {
	"max_steps": 100000,
	"n_part": 200,
	"sigma": 25},

    "track": ["origin", "bound", "directions", "samples", "extra_measure", "d_vec","poff_vec", "meas_each_axis", "vols_each_axis", "conditional_idx","vols_pinchoff","detected","r_vals","times","poff_traces"],
    "verbose": ["conditional_idx","vols_pinchoff","detected","r_vals"]
}

